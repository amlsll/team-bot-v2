modules = ["python-3.11"]

[env]
VIRTUAL_ENV = "/home/runner/${REPL_SLUG}/venv"
PATH = "${VIRTUAL_ENV}/bin"
PYTHONPATH = "${VIRTUAL_ENV}/lib/python3.11/site-packages"

[gitHubImport]
requiredFiles = [".replit", "requirements.txt"]

[deployment]
run = ["sh", "-c", "python replit_start.py"]

[[ports]]
localPort = 3000
externalPort = 80

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"
